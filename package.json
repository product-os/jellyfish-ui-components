{
  "name": "@balena/jellyfish-ui-components",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-ui-components.git"
  },
  "description": "UI components library for Jellyfish",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "ava": {
    "timeout": "2m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": true,
    "extensions": [
      "jsx"
    ],
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx"
        ]
      }
    },
    "files": [
      "lib/**/*.spec.{js,jsx}"
    ],
    "require": [
      "./test/browser-setup",
      "./test/register",
      "./test/max-listeners"
    ]
  },
  "scripts": {
    "lint": "eslint lib",
    "unit": "ava -v",
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run unit",
    "doc": "jsdoc2md --template doc/README.hbs lib/**/*.js > README.md"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-client-sdk": "^2.1.0",
    "@sentry/browser": "^5.20.1",
    "@webscopeio/react-textarea-autocomplete": "^4.7.1",
    "bluebird": "^3.7.2",
    "classnames": "^2.2.6",
    "color-hash": "^1.0.3",
    "common-tags": "^1.8.0",
    "copy-to-clipboard": "^3.3.1",
    "debounce-promise": "^3.1.2",
    "deep-copy": "^1.4.2",
    "detect-ua": "^1.0.0",
    "fast-equals": "^2.0.0",
    "fast-json-patch": "^3.0.0-1",
    "file-saver": "^2.0.2",
    "immutability-helper": "^3.1.1",
    "json-e": "^4.1.0",
    "lodash": "^4.17.19",
    "mark.js": "^8.11.1",
    "mixpanel-browser": "^2.38.0",
    "moment": "^2.27.0",
    "node-emoji": "^1.10.0",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dnd": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "react-textarea-autosize": "^7.1.2",
    "react-visibility-sensor": "^5.1.1",
    "rendition": "^16.2.0",
    "skhema": "^5.3.0",
    "styled-components": "^5.1.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^10.0.28",
    "ava": "^3.11.0",
    "browser-env": "^3.3.0",
    "catch-uncommitted": "^1.6.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.1.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc-to-markdown": "^6.0.1",
    "node-hook": "^1.0.0",
    "react-dnd-html5-backend": "^11.1.3",
    "redux-mock-store": "^1.5.4",
    "sinon": "^9.0.2"
  }
}
